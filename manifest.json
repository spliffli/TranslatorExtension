{
    "manifest_version": 2,
    "name": "Translator Extension",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.6.0.min.js", "content.js"]
      }
    ],
    "browser_action": {
        "default_icon": "icon.png"
      }
  }